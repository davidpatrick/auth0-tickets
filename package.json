    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.0",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.11.5",
    "eslint": "^3.1.1",
    "eslint-plugin-import": "^1.11.1",
    "eslint-plugin-react": "^5.2.2",
    "html-webpack-plugin": "^2.22.0",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.13.1",
