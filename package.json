{
  "name": "auth0-tickets",
  "description": "Submit customer support tickets to Zendesk",
  "version": "1.0.0",
  "engines": {
    "node": "6.2.2"
  },
  "scripts": {
    "build": "npm run build-app && npm run build-server && npm run build-server-src",
    "build-app": "webpack --config webpack.prod.config.babel.js --progress -p",
    "build-server": "babel server.prod.js -d dist -D --presets es2015",
    "build-server-src": "babel ./server/ -d ./dist/server/ -D --presets es2015",
    "dev": "babel-node server.dev",
    "start": "npm run build && node ./dist/server.prod",
    "test": "mocha --compilers js:babel-register --recursive --require ./test/setup.js",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "auth0-lock": "^10.0.0",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.11.5",
    "css-loader": "^0.23.1",
    "express": "^4.14.0",
    "express-jwt": "^3.4.0",
    "html-webpack-plugin": "^2.22.0",
    "isomorphic-fetch": "^2.2.1",
    "node-sass": "^3.8.0",
    "node-zendesk": "^1.1.10",
    "react": "^15.3.0",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "enzyme": "^2.4.1",
    "eslint": "^3.1.1",
    "eslint-plugin-import": "^1.11.1",
    "eslint-plugin-react": "^5.2.2",
    "expect": "^1.20.2",
    "mocha": "^3.0.0",
    "nock": "^8.0.0",
    "react-addons-test-utils": "^15.3.0",
    "react-hot-loader": "^1.3.0",
    "redux-mock-store": "^1.1.2",
    "sinon": "^1.17.5",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.1"
  },
  "author": "David Patrick",
  "license": "MIT"
}
